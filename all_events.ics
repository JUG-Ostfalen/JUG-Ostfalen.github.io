---
layout: none
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:{{site.baseurl}}
METHOD:PUBLISH
CALSCALE:GREGORIAN

BEGIN:VTIMEZONE
TZID:Europe/Berlin
X-LIC-LOCATION:Europe/Berlin
BEGIN:DAYLIGHT
TZOFFSETFROM:+0100
TZOFFSETTO:+0200
TZNAME:CEST
DTSTART:19810329T020000
RRULE:FREQ=YEARLY;BYMONTH=3;BYDAY=-1SU
END:DAYLIGHT
BEGIN:STANDARD
TZOFFSETFROM:+0200
TZOFFSETTO:+0100
TZNAME:CET
DTSTART:19961027T030000
RRULE:FREQ=YEARLY;BYMONTH=10;BYDAY=-1SU
END:STANDARD
END:VTIMEZONE

{% for post in site.categories.event limit:10 %}{% if site.time < post.date %}BEGIN:VEVENT
UID:{{ post.date | date: "%Y%m%d" }}@jug-ostfalen.de
{% assign default_label = "external" %}
{% assign default_contact = "none@void.com" %}
{% if post.organizer %}
  {% assign org = site.orgs[post.organizer] %}
  {% if org %}
    {% assign org_label = org.label | default: default_label %}
    {% assign org_contact = org.contact | default: default_contact %}
  {% else %}
    {% assign org_label = default_label %}
    {% assign org_contact = default_contact %}
  {% endif %}
{% else %}
  {% assign org_label = default_label %}
  {% assign org_contact = default_contact %}
{% endif %}
ORGANIZER;CN="{{ org_label }}":mailto:{{ org_contact }}
LOCATION:{% if site.locations[post.location] %}{{ site.locations[post.location].name }}\; {{ site.locations[post.location].street }}\; {{ site.locations[post.location].city }}{% else %}{{ post.location }}{% endif %}
SUMMARY:{{ post.title }} ({{ post.speaker }})
DESCRIPTION: Details: {{ post.url | prepend: site.url }}
CLASS:PUBLIC
DTSTART;TZID=Europe/Berlin:{{ post.date | date: "%Y%m%dT%H%M" }}00
DTEND;TZID=Europe/Berlin:{{ post.date   | date: "%Y%m%dT" }}210000
DTSTAMP:{{ site.time | date: "%Y%m%dT%H%M%S" }}Z
END:VEVENT
{% endif %}{% endfor %}END:VCALENDAR
